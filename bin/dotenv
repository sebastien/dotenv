#!/usr/bin/env bash
BASE=$(dirname "$(readlink -f "${BASH_SOURCE[0]}")")
source "$BASE/../share/dotenv/commands.bash"
COMMAND=$(basename $0)
case $COMMAND in
"dotenv"|"dotenv-manage")
	DEFINED=1
	;;
esac
if [ "$DEFINED" = 1 ]; then
	eval "command-$COMMAND $*"
else
	echo "dotenv: unknown command $COMMAND"
fi
# EOF
